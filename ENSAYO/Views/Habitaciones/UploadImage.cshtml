@model ENSAYO.Models.ImagenHabitacion

<h2>Subir una imagen para la habitación: @Model.IdHabitacion</h2>

<form asp-action="UploadImage" method="post" enctype="multipart/form-data">
    <div>
        <label for="file">Seleccione una imagen:</label>
        <input type="file" id="file" name="file" required />
    </div>

    <button type="submit">Subir Imagen</button>
</form>
